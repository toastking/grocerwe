doctype html
html(lang="en")
	head
		title= title
	body
		h1 To Do List
		#header
		form(action = 'new-task' method = 'post')
			input#form(type='text' placeholder = 'What would you like to do?' name='todo')
			input#btn(type='submit' value = 'Submit')
		#table
			ol#list
			each val in taskslist
				li #{val.text}
				